<h1>Listado de Articulos</h1>
  <%= link_to 'Nuevo Articulo', new_article_path %>
<table>
  <tr>
    <th>Articulo</th>
    <th>Descripci√≥n</th>
    <th colspan="3"></th>
  </tr>
 
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.text %></td>
      <td><%= link_to 'Visualizar', article_path(article) %></td>
      <td><%= link_to 'Editar', edit_article_path(article) %></td>
      <td><%= link_to 'Eliminar', article_path(article),
